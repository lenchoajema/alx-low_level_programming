#include "main.h"
/**
 * print_alphabet: function to print alphabet
 * Description: print lower
 * main: Entry point
 * Return - 0 on success
 */
void print_alphabet(void)
{
int i = 97;
char ch[26];
for (i; i < 124; i++)
{
putchar(char[i]);
}
putchar('\n');
}
int main(void)
{
print_alphabet(void);
